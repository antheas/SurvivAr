{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-react",
    "tslint-react-hooks",
    "tslint-react-native",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "curly": false,
    "interface-name": false,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  }
}
